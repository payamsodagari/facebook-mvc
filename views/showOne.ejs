<%- include('./components/header') %>

<% if (result) { %>     
    <div class="card">
        <div class="card-header">
          <%= result._id %>
        </div>
        <div class="card-body">
          <h5 class="card-title"><%= result.name %></h5>
          <p class="card-text"><%= result.message %></p>
          <a href=/edit-feed/<%= result._id %>  class="btn btn-primary">Update</a>
          <a href=/delete-feed/<%= result._id %>  class="btn btn-warning">Delete</a>
        </div>
      </div>
          
    <% } else { %>
        there is no record
    <% } %>

<%- include('./components/footer') %>
